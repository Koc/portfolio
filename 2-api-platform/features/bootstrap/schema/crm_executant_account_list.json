{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "required": true,
  "properties": {
    "@context": {
      "type": "string",
      "required": true
    },
    "@id": {
      "type": "string",
      "required": true
    },
    "@type": {
      "type": "string",
      "required": true
    },
    "hydra:member": {
      "type": "array",
      "required": true,
      "items": {
        "type": "object",
        "required": true,
        "properties": {
          "@id": {
            "type": "string",
            "required": true
          },
          "@type": {
            "type": "string",
            "required": true
          },
          "currency": {
            "type": ["null", "string"],
            "required": true
          },
          "executant": {
            "type": "string",
            "required": true
          },
          "id": {
            "type": "number",
            "required": true
          },
          "internalName": {
            "type": "string",
            "required": true
          },
          "translations": {
            "type": "object",
            "required": true,
            "properties": {
              "ru": {
                "type": "object",
                "required": true,
                "properties": {
                  "@id": {
                    "type": "string",
                    "required": true
                  },
                  "@type": {
                    "type": "string",
                    "required": true
                  },
                  "details": {
                    "type": ["null", "string"],
                    "required": true
                  },
                  "id": {
                    "type": "number",
                    "required": true
                  },
                  "locale": {
                    "type": "string",
                    "required": true
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
